<% content_for :body_content_left do %>
  <div id="teams">
    <% @teams.each do |team| %>

      	<%= image_fu(team.image,'400x291#c') %>
        <h2><%=raw team.name %> - <%=raw team.title %></h2>
        <p><%=raw team.body %></p>

    <% end %>
  </div>
<% end %>

<%= render '/refinery/content_page' %>
